<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.sistemamatricula.Controlador.CreacionAula">
   <children>
       <GridPane layoutX="50" layoutY="40"
                 hgap="10" vgap="15">

           <!-- Nombres -->
           <Label text="Nombre:" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
           <TextField fx:id="txtNombre"
                      GridPane.rowIndex="0" GridPane.columnIndex="1"
                      prefWidth="250"/>

           <!-- Apellidos -->
           <Label text="capacidad:" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
           <TextField fx:id="txtCapacidad"
                      GridPane.rowIndex="1" GridPane.columnIndex="1"/>



           <!-- Botón -->
           <Button text="Registrar"
                   GridPane.rowIndex="4"
                   GridPane.columnIndex="1"
                   onAction="#registrarEstudiante"
           />
           <Button text="Atrás"
                   GridPane.rowIndex="4"
                   GridPane.columnIndex="2"
                   onAction="#retroceder"
           />



       </GridPane>
   </children>
</AnchorPane>
